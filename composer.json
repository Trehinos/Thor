{
  "name": "trehinos/thor",
  "description": "Smart framework for PHP-8.1",
  "version": "0.9.6",
  "type": "project",
  "keywords": ["php8.1", "framework", "http", "daemon"],
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "SÃ©bastien GELDREICH",
      "email": "trehinos@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Thor\\": "thor/",
      "App\\": "app/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "vendor-dir": "vendors/",
    "bin-dir": "bin/"
  },
  "require": {
    "php": "^8.2",
    "ext-calendar": "*",
    "ext-curl": "*",
    "ext-ftp": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "ext-readline": "*",
    "phpoffice/phpspreadsheet": "1.*",
    "datatables.net/editor-php": "2.*",
    "symfony/var-dumper": "5.*",
    "symfony/yaml": "5.*",
    "twig/twig": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  }
}
